body
	margin 0
	min-width 836px
	background-color #fafafa
	font 14px 'micsoft yahei', 'Helvetica Neue', Helvetica, Arial, sans-serif

h2
	margin 0
	font-size 12px

a
	color #000
	text-decoration none

input
  outline none

button
	margin 0
	padding 0
	border 0
	background none
	font-size 100%
	vertical-align baseline
	font-family inherit
	font-weight inherit
	color inherit
	outline 0

ul
	padding 0
	margin 0
	list-style none